<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFF"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rundefined_rl_rl"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <ImageView
            android:id="@+id/rundefined"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:src="@mipmap/ic_launcher"
            android:scaleType="fitXY"/>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ScrollView
                android:id="@+id/rr8naa7kr36r"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/remp9f4hi3ev"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center_horizontal"
                        android:background="@drawable/lr27000000000000000"
                        android:paddingTop="216dp"
                        android:paddingBottom="32dp"
                        android:paddingHorizontal="20dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:id="@+id/rjodxej589j"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:paddingRight="4dp"
                            android:layout_marginBottom="152dp"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:id="@+id/rzy4x8ixblze"
                                android:layout_width="36dp"
                                android:layout_height="36dp"
                                android:background="#595959"
                                android:layout_marginEnd="12dp"
                                android:orientation="vertical">
                            </LinearLayout>

                            <TextView
                                android:id="@+id/rq0ogfwl36ir"
                                android:textColor="#FFFFFF"
                                android:textSize="36sp"
                                android:textStyle="bold"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:text="Munonton"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/rdrhip4aa3g"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="40dp"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/rkmz1fnyzfxb"
                                android:textColor="#FFFFFF"
                                android:textSize="36sp"
                                android:textStyle="bold"
                                android:layout_marginBottom="8dp"
                                android:layout_height="wrap_content"
                                android:layout_width="wrap_content"
                                android:text="Get Started"/>

                            <TextView
                                android:id="@+id/rfcslrbs40ju"
                                android:textColor="#B9BFC1"
                                android:textSize="20sp"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:layout_weight="1"
                                android:text="Come in to enjoy exclusive shows at the cinema with an amazing experience"/>
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/rvyxss619ks"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="52dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:id="@+id/rol8crfvwycf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:background="@drawable/cr8bf0f0f0"
                                android:paddingVertical="12dp"
                                android:layout_marginBottom="24dp"
                                android:clickable="true"
                                android:focusable="true"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/rkilrxz69ake"
                                    android:textColor="#191B1C"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Continue with email"/>
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/rw63jli1lfn"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center_horizontal"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/r5d8l91iccgx"
                                    android:textColor="#B9BFC1"
                                    android:textSize="16sp"
                                    android:textStyle="bold"
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:text="Sign in"/>
                            </LinearLayout>
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/rzwbaefbjbnb"
                            android:layout_width="120dp"
                            android:layout_height="1dp"
                            android:src="@mipmap/ic_launcher"
                            android:scaleType="fitXY"/>
                    </LinearLayout>
                </LinearLayout>
            </ScrollView>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>